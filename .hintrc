{
  "extends": [
    "development"
  ],
  "hints": {
    "apple-touch-icons": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter"
        ]
      }
    ],
    "css-prefix-order": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 116",
    "not firefox <= 116",
    "not opera <= 101"
  ]
}